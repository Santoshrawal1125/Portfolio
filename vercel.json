{
    "builds": [
      {
        "src": "portfolio/wsgi.py",
        "use": "@vercel/python"
      },
      {
        "use": "@vercel/static-build",
        "config": { "distDir": "portfolio/static" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "portfolio/wsgi.py"
      }
    ],
    "outputDirectory": "staticfiles_build"
}